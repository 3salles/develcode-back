// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User{
  Codigo String @id @default(uuid())
  Nome String 
  Dtnasci DateTime @default(now())
  Foto String

  @@map("user")
}